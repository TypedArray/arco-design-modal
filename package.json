{
  "name": "@typedarray/arco-design-modal",
  "author": "lomocc",
  "version": "0.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/TypedArray/arco-design-modal"
  },
  "bugs": {
    "url": "https://github.com/TypedArray/arco-design-modal/issues"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "gh-pages": "gh-pages -d storybook-static"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/arco-design-modal.esm.js",
  "size-limit": [
    {
      "path": "dist/arco-design-modal.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/arco-design-modal.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@arco-design/web-react": "^2.25.1",
    "@babel/core": "^7.16.0",
    "@size-limit/preset-small-lib": "^7.0.3",
    "@storybook/addon-essentials": "^6.4.3",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.4.3",
    "@storybook/addons": "^6.4.3",
    "@storybook/react": "^6.4.3",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "^8.2.3",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "size-limit": "^7.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  }
}
